ZSH_THEME="agnoster"
# --- Plugins

plugins=(git 
          poetry
          zsh-syntax-highlighting 
          zsh-autosuggestions 
          vi-mode)

# --- Source 
source $ZSH/oh-my-zsh.sh
source $HOME/.functions
source $HOME/.git-functions
source $HOME/.aliases
source /opt/homebrew/share/zsh-syntax-highlighting
source /opt/homebrew/share/zsh-autosuggestions

source $HOME/.zshenv_local

export PATH="$HOME/.poetry/bin:$PATH"

# The next line updates PATH for the Google Cloud SDK.
if [ -f '/Users/mwasilewski/Downloads/google-cloud-sdk/path.zsh.inc' ]; then . '/Users/mwasilewski/Downloads/google-cloud-sdk/path.zsh.inc'; fi

# The next line enables shell command completion for gcloud.
if [ -f '/Users/mwasilewski/Downloads/google-cloud-sdk/completion.zsh.inc' ]; then . '/Users/mwasilewski/Downloads/google-cloud-sdk/completion.zsh.inc'; fi
